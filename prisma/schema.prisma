generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum RoleTypes {
  Admin
  Assistant
}

model User {
  id        String @default(uuid()) @id
  name      String
  password  String
  role      RoleTypes
}